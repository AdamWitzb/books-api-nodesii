{
  "engines": {
    "node": "^14"
  },
  "name": "books-api",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "test": "mocha --exit -r ts-node/register test/**/*.e2e.ts",
    "start:dev": "ts-node-dev --debug --respawn src/server.ts",
    "docker:debug:run": "docker-compose up --build",
    "docker:prod:build": "docker build -f docker/Dockerfile .",
    "format": "prettier --write src/ test/"
  },
  "dependencies": {
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/mocha": "8.2.0",
    "@types/supertest": "2.0.10",
    "mocha": "8.2.1",
    "prettier": "2.2.1",
    "supertest": "6.1.1",
    "ts-node-dev": "1.1.0",
    "typescript": "4.1.3"
  }
}
